{
    "compilerOptions":
    {
        "target":"ES2022",
        "module": "CommonJs",
        "sourceMap":true,
        "removeComments": true,
        "strict":true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noImplicitReturns": false
        
    }
}